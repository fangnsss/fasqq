px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"name\" label=\"分类名称\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"description\" label=\"分类描述\">\r\n          <el-input v-model=\"form.description\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"分类图标\">\r\n          <el-upload\r\n              class=\"avatar-uploader\"\r\n              :action=\"$baseUrl + '/files/upload'\"\r\n              :headers=\"{ token: user.token }\"\r\n              list-type=\"picture\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n          >\r\n            <el-button type=\"primary\">上传图标</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Collect from \"@/views/front/Collect.vue\";\r\n\r\nexport default {\r\n  computed: {\r\n    Collect() {\r\n      return Collect\r\n    }\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      addressData:[],\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      formVisible:false,\r\n      form:{}\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadCollect(1)\r\n\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    addAddress(){\r\n      this.form={}\r\n      this.formVisible=true\r\n\r\n    },\r\n    loadCollect(pageNum){\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/address/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        if (res.code==='200'){\r\n          this.addressData = res.data?.list\r\n          this.total = res.data?.total\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n    navTo(url) {\r\n      location.href = url\r\n    },\r\n    del(id){\r\n      this.$request.delete('/address/delete/' +id).then(res=>{\r\n        if (res.code==='200'){\r\n          this.$message.success('删除成功')\r\n          this.loadCollect(1)\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.loadCollect(pageNum)\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAoEA,OAAAA,OAAA;AAEA;EACAC,QAAA;IACAD,QAAA;MACA,OAAAA,OAAA;IACA;EACA;EAEAE,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,WAAA;EAEA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAJ,IAAA;MACA,KAAAD,WAAA;IAEA;IACAG,YAAAN,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAS,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAX,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAf,WAAA,GAAAc,GAAA,CAAApB,IAAA,EAAAsB,IAAA;UACA,KAAAb,KAAA,GAAAW,GAAA,CAAApB,IAAA,EAAAS,KAAA;QACA;UACA,KAAAc,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MAEA;IACA;IACAC,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAG,IAAAC,EAAA;MACA,KAAAf,QAAA,CAAAgB,MAAA,sBAAAD,EAAA,EAAAZ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAE,QAAA,CAAAU,OAAA;UACA,KAAApB,WAAA;QACA;UACA,KAAAU,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;IAEA;IACAS,oBAAA3B,OAAA;MACA,KAAAM,WAAA,CAAAN,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}