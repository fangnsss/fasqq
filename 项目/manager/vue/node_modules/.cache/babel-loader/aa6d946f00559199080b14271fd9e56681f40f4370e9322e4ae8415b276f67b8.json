{"ast":null,"code":"import Collect from \"@/views/front/Collect.vue\";\nexport default {\n  computed: {\n    Collect() {\n      return Collect;\n    }\n  },\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      addressData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          name: '请输入收货人',\n          trigger: 'blur'\n        }],\n        useraddress: [{\n          required: true,\n          name: '请输入收货地址',\n          trigger: 'blur'\n        }],\n        phone: [{\n          required: true,\n          name: '请输入联系电话',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  mounted() {\n    this.loadAddress(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    addAddress() {\n      this.form = {};\n      this.formVisible = true;\n    },\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/address/update' : '/address/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 表示成功保存\n              this.$message.success('保存成功');\n              this.loadAddress(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    loadAddress(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/address/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.addressData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    navTo(url) {\n      location.href = url;\n    },\n    del(id) {\n      this.$request.delete('/address/delete/' + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success('删除成功');\n          this.loadCollect(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.loadCollect(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["Collect","computed","data","user","JSON","parse","localStorage","getItem","addressData","pageNum","pageSize","total","formVisible","form","rules","username","required","name","trigger","useraddress","phone","mounted","loadAddress","methods","addAddress","save","$refs","formRef","validate","valid","$request","url","id","method","then","res","code","$message","success","fromVisible","error","msg","get","params","list","navTo","location","href","del","delete","loadCollect","handleCurrentChange"],"sources":["src/views/front/Address.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width: 70%; background-color: white; margin: 30px auto; border-radius: 20px\">\r\n      <div style=\"border-bottom: 10px\">\r\n        <div style=\"display:flex; font-size: 18px; color: #000000FF; line-height: 80px; border-bottom: #cccccc 1px solid\">\r\n          <div style=\"flex: 3; margin-left: 20px\">我的地址</div>\r\n          <div style=\"flex: 1;text-align: right;padding-right: 20px\">\r\n            <el-button type=\"warning\" @click=\"addAddress\">添加收货地址</el-button>\r\n          </div>\r\n        </div>\r\n        <div style=\"margin: 20px 0;padding: 0 50px;\">\r\n          <div class=\"table\">\r\n            <el-table :data=\"addressData\" strip>\r\n              <el-table-column prop=\"username\" label=\"收货人\" width=\"300px\"></el-table-column>\r\n              <el-table-column prop=\"useraddress\" label=\"收货地址\"></el-table-column>\r\n              <el-table-column prop=\"phone\" label=\"联系电话\"></el-table-column>\r\n              <el-table-column label=\"操作\" align=\"center\" width=\"180\">\r\n                <template v-slot=\"scope\">\r\n                  <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">编辑地址</el-button>\r\n                  <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">删除地址</el-button>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n\r\n            <div class=\"pagination\" style=\"margin-top: 20px\">\r\n              <el-pagination\r\n                  background\r\n                  @current-change=\"handleCurrentChange\"\r\n                  :current-page=\"pageNum\"\r\n                  :page-sizes=\"[5, 10, 20]\"\r\n                  :page-size=\"pageSize\"\r\n                  layout=\"total, prev, pager, next\"\r\n                  :total=\"total\">\r\n              </el-pagination>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <el-dialog title=\"地址信息\" :visible.sync=\"formVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"username\" label=\"收货人\">\r\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"useraddress\" label=\"收货地址\">\r\n          <el-input v-model=\"form.useraddress\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"phone\" label=\"联系电话\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"formVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Collect from \"@/views/front/Collect.vue\";\r\n\r\nexport default {\r\n  computed: {\r\n    Collect() {\r\n      return Collect\r\n    }\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      addressData:[],\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      formVisible:false,\r\n      form:{},\r\n      rules: {\r\n        username: [\r\n          {required: true, name: '请输入收货人', trigger: 'blur'},\r\n        ],\r\n        useraddress: [\r\n          {required: true, name: '请输入收货地址', trigger: 'blur'},\r\n        ],\r\n        phone: [\r\n          {required: true, name: '请输入联系电话', trigger: 'blur'},\r\n        ],\r\n      },\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadAddress(1)\r\n\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    addAddress(){\r\n      this.form={}\r\n      this.formVisible=true\r\n\r\n    },\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$request({\r\n            url: this.form.id ? '/address/update' : '/address/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('保存成功')\r\n              this.loadAddress(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    loadAddress(pageNum){\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/address/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        if (res.code==='200'){\r\n          this.addressData = res.data?.list\r\n          this.total = res.data?.total\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n    navTo(url) {\r\n      location.href = url\r\n    },\r\n    del(id){\r\n      this.$request.delete('/address/delete/' +id).then(res=>{\r\n        if (res.code==='200'){\r\n          this.$message.success('删除成功')\r\n          this.loadCollect(1)\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.loadCollect(pageNum)\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA6DA,OAAAA,OAAA;AAEA;EACAC,QAAA;IACAD,QAAA;MACA,OAAAA,OAAA;IACA;EACA;EAEAE,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,IAAA;UAAAC,OAAA;QAAA,EACA;QACAC,WAAA,GACA;UAAAH,QAAA;UAAAC,IAAA;UAAAC,OAAA;QAAA,EACA;QACAE,KAAA,GACA;UAAAJ,QAAA;UAAAC,IAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACA,KAAAC,WAAA;EAEA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAX,IAAA;MACA,KAAAD,WAAA;IAEA;IACAa,KAAA;MAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,QAAA;YACAC,GAAA,OAAAlB,IAAA,CAAAmB,EAAA;YACAC,MAAA,OAAApB,IAAA,CAAAmB,EAAA;YACA9B,IAAA,OAAAW;UACA,GAAAqB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAAhB,WAAA;cACA,KAAAiB,WAAA;YACA;cACA,KAAAF,QAAA,CAAAG,KAAA,CAAAL,GAAA,CAAAM,GAAA;YACA;UACA;QACA;MACA;IACA;;IACAnB,YAAAb,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAqB,QAAA,CAAAY,GAAA;QACAC,MAAA;UACAlC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAwB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA5B,WAAA,GAAA2B,GAAA,CAAAjC,IAAA,EAAA0C,IAAA;UACA,KAAAjC,KAAA,GAAAwB,GAAA,CAAAjC,IAAA,EAAAS,KAAA;QACA;UACA,KAAA0B,QAAA,CAAAG,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MAEA;IACA;IACAI,MAAAd,