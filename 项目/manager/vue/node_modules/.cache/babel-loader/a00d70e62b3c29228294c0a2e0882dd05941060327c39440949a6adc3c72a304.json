{"ast":null,"code":"import Collect from \"@/views/front/Collect.vue\";\nexport default {\n  computed: {\n    Collect() {\n      return Collect;\n    }\n  },\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      CollectData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0\n    };\n  },\n  mounted() {\n    this.loadCollect(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    loadCollect(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/collect/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    navTo(url) {\n      location.href = url;\n    },\n    del(id) {},\n    handleCurrentChange(pageNum) {\n      this.loadCollect(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["Collect","computed","data","user","JSON","parse","localStorage","getItem","CollectData","pageNum","pageSize","total","mounted","loadCollect","methods","$request","get","params","then","res","tableData","list","navTo","url","location","href","del","id","handleCurrentChange"],"sources":["src/views/front/Collect.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width: 70%; background-color: white; margin: 30px auto; border-radius: 20px\">\r\n      <div>\r\n        <div style=\"font-size: 18px; color: #000000FF; line-height: 80px; border-bottom: #cccccc 1px solid\">全部收藏</div>\r\n        <div style=\"margin: 20px 0\">\r\n          <div class=\"table\">\r\n            <el-table :data=\"CollectData\" strip>\r\n              <el-table-column prop=\"id\" label=\"序号\" width=\"70\" align=\"center\" sortable></el-table-column>\r\n              <el-table-column prop=\"username\" label=\"账号\"></el-table-column>\r\n              <el-table-column prop=\"name