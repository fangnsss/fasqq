  pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadCollect(1)\r\n\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    loadCollect(pageNum){\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/collect/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        \r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    navTo(url) {\r\n      location.href = url\r\n    },\r\n    del(id){\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.loadCollect(pageNum)\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAiDA,OAAAA,OAAA;AAEA;EACAC,QAAA;IACAD,QAAA;MACA,OAAAA,OAAA;IACA;EACA;EAEAE,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,WAAA;EAEA;EACA;EACAC,OAAA;IACAD,YAAAJ,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAM,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAR,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAQ,IAAA,CAAAC,GAAA;QAEA,KAAAC,SAAA,GAAAD,GAAA,CAAAjB,IAAA,EAAAmB,IAAA;QACA,KAAAV,KAAA,GAAAQ,GAAA,CAAAjB,IAAA,EAAAS,KAAA;MACA;IACA;IACAW,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAG,IAAAC,EAAA,GAEA;IACAC,oBAAAnB,OAAA;MACA,KAAAI,WAAA,CAAAJ,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}